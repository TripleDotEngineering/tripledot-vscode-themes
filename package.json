{
  "name": "tdthemes",
  "displayName": "Triple Dot Themes",
  "description": "A collection of themes - including Monokai Neue with multi-level JSON highlighting",
  "version": "0.1.0",
  "publisher": "Triple Dot",
  "author": {
    "name": "Josh Kaplan"
  },
  "repository": "https://github.com/TripleDotEngineering/tripledot-vscode-themes",
  "license": "MIT",
  "engines": {
    "vscode": "^1.73.0"
  },
  "scripts": {
    "build": "node ./src/index.js"
  },
  "categories": [
    "Themes"
  ],
  "keywords": ["themes", "color scheme", "monokai", "monokai neue", "json"],
  "icon": "https://github.com/TripleDotEngineering/tripledot-vscode-themes/blob/main/icon.png",
  "contributes": {
    "themes": [
      {
        "label": "Monokai Neue",
        "uiTheme": "vs-dark",
        "path": "./themes/monokai-neue-color-theme.json"
      },
      {
        "label": "Triple Dot Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/tripledot-dark-color-theme.json"
      },
      {
        "label": "Triple Dot Light",
        "uiTheme": "vs",
        "path": "./themes/tripledot-light-color-theme.json"
      },
      {
        "label": "Microsoft",
        "uiTheme": "vs",
        "path": "./themes/microsoft-color-theme.json"
      },
      {
        "label": "Microsoft Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/microsoft-dark-color-theme.json"
      }
    ]
  }
}
